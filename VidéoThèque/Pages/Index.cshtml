@page
@model IndexModel
@{
	ViewData["Title"] = "Accueil";
}

<h1>Connexion</h1>
<div class="row">
	<div class="col-md-4">
		<form method="post">
			<div class="form-group">
				<label asp-for="User.Identifiant" class="control-label"></label>
				<input asp-for="User.Identifiant" class="form-control" />
				<span asp-validation-for="User.Identifiant" class="text-danger"></span>
			</div>
			<div class="form-group">
				<label asp-for="User.MotDePasse" class="control-label"></label>
				<input asp-for="User.MotDePasse" class="form-control" />
				<span asp-validation-for="User.MotDePasse" class="text-danger"></span>
			</div>
			<div class="form-group">
				<input type="submit" value="Se connecter" class="btn btn-primary" />
			</div>

		</form>
	</div>
</div>

<div>
	<a asp-page="./Users/Inscription">Vous inscrire ?</a>
</div>


